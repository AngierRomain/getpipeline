pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                 echo 'Hello Build !'
                 script{
                     def response = sh(script: 'curl --cookie-jar /home/session.txt -s http://deployhub.openmakesoftware.com/API/login?user=admin&pass=123456', returnStdout: true) 
                 }  
            }
        }

        stage('Test') { 
            steps {
                echo 'Hello Test !'
            }
        }

        stage('Deploy') { 
            steps {
                 echo 'Hello Deploy !'
            }
        }
    }
}

