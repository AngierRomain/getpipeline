pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                 echo 'Hello Build !'
                 script{
                     def response = sh(script: 'curl -b session -X POST http://deployhub.openmakesoftware.com/API/UserProfile?user=admin', returnStdout: true) 
                 }  
            }
        }

        stage('Test') { 
            steps {
                echo 'Hello Test !'
            }
        }

        stage('Deploy') { 
            steps {
                 echo 'Hello Deploy !'
            }
        }
    }
}

